#. module: auth_signup_approval
#: model:mail.template,body_html:auth_signup_approval.user_pending_approval
msgid "\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>Dear,</p>\n"
"    <p>\n"
"       The user ${object.name} has requested to sign up on the site. The request is awaiting your approval. <br/>\n"
"       <a href=${object.get_url_view_user()}>Click here</a> to go to the user config.\n"
"    </p>\n"
"    <p>Best regards,</p>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto; margin-top: 10px; background: #fff repeat top /100%;color:#777777\">\n"
"    ${user.signature | safe}\n"
"</div>"
msgstr "\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>Estimado,</p>\n"
"    <p>\n"
"       El usuario ${object.name} ha solicitado resgistrarse en el sitio web. La solicitud esta esperando su aprobación. <br/>\n"
"       <a href=${object.get_url_view_user()}>Click aquí</a> para ir a la configuracion del usuario.\n"
"    </p>\n"
"    <p>Saludos,</p>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto; margin-top: 10px; background: #fff repeat top /100%;color:#777777\">\n"
"    ${user.signature | safe}\n"
"</div>"

#. module: auth_signup_approval
#: model:mail.template,body_html:auth_signup_approval.user_signup_rejected
msgid "\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <table cellspacing=\"0\" cellpadding=\"0\" style=\"width:600px;border-collapse:collapse;background:inherit;color:inherit\">\n"
"        <tbody><tr>\n"
"            <td valign=\"center\" width=\"200\" style=\"padding:10px 10px 10px 5px;font-size: 12px\">\n"
"                <img src=\"/logo.png\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"${user.company_id.name}\" />\n"
"            </td>\n"
"        </tr></tbody>\n"
"    </table>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>Dear ${object.name},</p>\n"
"    <p>\n"
"    We're sorry, but your request to register in our site has been rejected.\n"
"    For any inquiry about the registration approval process, please contact us.\n"
"    </p>\n"
"    <p>Best regards,</p>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto; margin-top: 10px; background: #fff repeat top /100%;color:#777777\">\n"
"    ${user.signature | safe}\n"
"</div>"
msgstr "\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <table cellspacing=\"0\" cellpadding=\"0\" style=\"width:600px;border-collapse:collapse;background:inherit;color:inherit\">\n"
"        <tbody><tr>\n"
"            <td valign=\"center\" width=\"200\" style=\"padding:10px 10px 10px 5px;font-size: 12px\">\n"
"                <img src=\"/logo.png\" style=\"padding: 0px; margin: 0px; height: auto; width: 80px;\" alt=\"${user.company_id.name}\" />\n"
"            </td>\n"
"        </tr></tbody>\n"
"    </table>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto;background: #FFFFFF repeat top /100%;color:#777777\">\n"
"    <p>Estimado ${object.name},</p>\n"
"    <p>\n"
"    Lo sentimos, pero su solicitud de registro en nuestro sitio ha sido rechazada.\n"
"    Para cualquier consulta sobre el proceso de aprobación de registro, por favor contáctenos.\n"
"    </p>\n"
"    <p>Saludos,</p>\n"
"</div>\n"
"<div style=\"padding:0px;width:600px;margin:auto; margin-top: 10px; background: #fff repeat top /100%;color:#777777\">\n"
"    ${user.signature | safe}\n"
"</div>"

